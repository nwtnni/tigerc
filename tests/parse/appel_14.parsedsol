(
  let
  (
    (
      (type arrtype = array of int)
      (type rectype = (
        name : string
        id : int
      ))
    )
    (var rec := (
      rectype (
        name = "aname"
        id = 0
      )
    ))
    (var arr := (arrtype size 3 of 0))
  )
  in
  ((if (rec <> arr) then 3 else 4))
)

